<% if @symbol %> 

 <% if @quote["Response"] %> 
 
     <h4>That Crypto Symbol does not exciste. Please try again...</h4>
 
 <% else %>

<% @quote["DISPLAY"].each do |key, value| %>

<div class="jumbotron">
  <h1 class="display-4"><%= key%></h1>
  Price: <%=value ["USD"]["PRICE"]%><br/>
  Last Trade ID: <%=value ["USD"]["LASTTRADEID"]%><br/>
    24Hr High: <%=value ["USD"]["HIGHDAY"]%><br/>
    24Hr Low: <%=value ["USD"]["LOWDAY"]%><br/>
    Market Cap: <%=value ["USD"]["MKTCAP"]%><br/>
    LAST MARKET: <%=value ["USD"]["LASTMARKET"]%><br/>
</div>

<% end %>

<% end %>

<% else %>

    <h4> Please Enter a crypto symbol in the form above...</h4>
    
<% end %>